@using AgrifoodManagement.Web.Views.Presentation.Resources;

<section class="team section-padding" data-scroll-index="3">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="sectioner-header text-center">
                    <h3>@_MissionResources.Title</h3>
                    <span class="line"></span>
                    <p>@_MissionResources.Header</p>
                </div>
                <div class="section-content text-center">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="team-detail wow bounce" data-wow-delay="0.2s">
                                <img src="images/user1.jpg" class="img-fluid" />
                                <h4>@_MissionResources.Mission1Title</h4>
                                <p>@_MissionResources.Mission1Description</p>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="team-detail wow bounce" data-wow-delay="0.4s">
                                <img src="images/user2.jpg" class="img-fluid" />
                                <h4>@_MissionResources.Mission2Title</h4>
                                <p>@_MissionResources.Mission2Description</p>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="team-detail wow bounce" data-wow-delay="0.6s">
                                <img src="images/user3.jpg" class="img-fluid" />
                                <h4>@_MissionResources.Mission3Title</h4>
                                <p>@_MissionResources.Mission3Description</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
